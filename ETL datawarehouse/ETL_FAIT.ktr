    <sql>SELECT
  CodeMagasin
, NomMagasinConcurrent
, AdresseMagasinConcurrent
, CodePostalMagasinConcurrent
, VilleMagasinConcurrent
, NomEnseigne
, CodeProduit
, TypeCommanditaire
, OffreNiveau1
, OffreNiveau2
, OffreNiveau3
, NumTarif
, DateRelevé
, NotrePrix
, PrixConcurrent
, ROUND(((NotrePrix-PrixConcurrent)/NotrePrix)*100)as Ratio_Ecart_prix_pourcentage 
,CASE
    WHEN ((NotrePrix-PrixConcurrent)/NotrePrix)*100 = 0 THEN "identique"
    WHEN ((NotrePrix-PrixConcurrent)/NotrePrix)*100 > 0 THEN "positive"
    ELSE "negative"
END
 
AS type_ecart_prix
FROM basetransaction.releves
</sql>
